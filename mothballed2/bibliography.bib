	Mesh = {Animals; Brain Mapping; Color Perception; Dominance, Cerebral; Electron Transport Complex IV; Electrophysiology; Macaca fascicularis; Models, Neurological; Orientation; Photic Stimulation; Visual Cortex; Visual Fields; Visual Perception},
	Month = {May},
