@article{Raidvee:2011ve,
	Abstract = {Six observers were asked to indicate in which of two opposite directions, to the right or to the left, an entire display appeared to move, based on the proportion of right vs leftward motion elements, each of which was distinctly visible. The performance of each observer was described by Thurstone's discriminative processes and Bernoulli trial models which described empirical psychometric functions equally well. Although formally it was impossible to discriminate between these two models, treating observer as a counting device which measures a randomly selected subsample of all available motion elements had certain advantages. According to the Bernoulli trial model decisions about the global motion direction in a range of 12-800 elements were based on taking into account about 4$\pm$2 random moving dot elements. This small number is not due to cancellation of the opposite motion vectors since the motion direction recognition performance did not improve after the compared motion directions were made orthogonal. This may indicate that the motion pooling mechanism studied in our experiment is strongly limited in capacity.},
	Author = {Raidvee, Aire and Averin, Kristiina and Kreegipuu, Kairi and Allik, J{\"u}ri},
	Date-Added = {2011-08-19 02:41:57 -0700},
