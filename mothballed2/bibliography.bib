	Journal = {NeuroImage},
	Keywords = {Retinotopic mapping; Linear cortical magnification; Laplace-Beltrami operator; Dynamic programming},
	Number = {1},
	Pages = {125--138},
	Title = {Estimating linear cortical magnification in human primary visual cortex via dynamic programming},
	Ty = {JOUR},
	Url = {http://www.sciencedirect.com/science/article/B6WNP-4J7300B-1/2/8b52c730f16dcfa477de2cf91d4e4c68},
