	Mesh = {Animals; Autoradiography; Cerebral Cortex; Corpus Callosum; Female; Functional Laterality; Geniculate Bodies; Horseradish Peroxidase; Macaca; Macaca fascicularis; Male; Visual Cortex; Visual Pathways},
	Month = {Feb},
	Number = {4},
	Pages = {451-80},
	Pmid = {3958238},
	Title = {The projections from striate cortex (V1) to areas V2 and V3 in the macaque monkey: asymmetries, areal boundaries, and patchy connections},
