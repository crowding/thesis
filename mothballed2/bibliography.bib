@article{Zeki:2009:J-Physiol:19525567,
	Abstract = {'To determine orientation we occasionally used a PDP-12 computer to produce a graph of average response vs orientation, generating the slit electronically on a television screen. This method took much longer, and the usual minute-to-minute variations in responsiveness of the cell tended to make the curves broader and noisier. We concluded that for both speed and for precision it is hard to beat judgments based on the human ear. Certainly [our curves] could not have been obtained with computer averaging methods before the authors reached the age of mandatory retirement.' (Hubel \& Wiesel, 1974).},
	Author = {Zeki, S},
