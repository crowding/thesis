	Mesh = {Animals; Brain Mapping; Cerebral Cortex; Evoked Potentials, Visual; Figural Aftereffect; Humans; Magnetic Resonance Imaging; Motion Perception; Nerve Net; Neurons; Optical Illusions; Orientation; Pattern Recognition, Visual; Psychophysics; Transcranial Magnetic Stimulation; Visual Pathways},
	Month = {Dec},
	Number = {12},
	Pages = {481-7},
	Pmc = {PMC3087115},
	Pmid = {18951829},
	Pst = {ppublish},
	Title = {The motion aftereffect reloaded},
	Volume = {12},
	Year = {2008},
